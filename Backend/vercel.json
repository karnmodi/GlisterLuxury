{
  "version": 2,
  "buildCommand": "echo 'No build needed for Express API'",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "regions": ["lhr1"],
  "env": {
    "NODE_ENV": "production"
  }
}
