{
  "version": 2,
  "buildCommand": "echo 'No build needed for Express API'",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "regions": ["lhr1"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
